@page
@model SchulungQotd.Mvc.Pages.Quote.IndexModel

<table class="table table-hover">
    <thead>
        <tr>
            <th>Spruch</th>
            <th>Autor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var quote in Model.Quotes)
        {
            <tr>
                <td>@quote.QuoteText</td>
                <td>@quote.AuthorName</td>
            </tr>
        }
    </tbody>
</table>